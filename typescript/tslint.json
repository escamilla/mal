{
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "no-console": false,
    "member-access": [
      true,
      "check-accessor",
      "check-constructor"
    ],
    "typedef": [
      true,
      "call-signature",
      "arrow-call-signature",
      "parameter",
      "arrow-parameter",
      "property-declaration",
      "variable-declaration",
      "member-variable-declaration",
      "object-destructuring",
      "array-destructuring"
    ],
    "max-classes-per-file": false
  }
}
