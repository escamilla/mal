{
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "member-access": [
      true,
      "check-accessor",
      "check-constructor"
    ],
    "prefer-switch": true,
    "typedef": [
      true,
      "call-signature",
      "arrow-call-signature",
      "parameter",
      "arrow-parameter",
      "property-declaration",
      "variable-declaration",
      "member-variable-declaration",
      "object-destructuring",
      "array-destructuring"
    ]
  }
}
